{
  "manifest_version": 2,
  "name": "E2T | Exploratory Testing Tool",
  "version": "0.3",
  "description": "Use javascript events for Exploratory Testing quality measurement.",
  "permissions": [
    "webNavigation",
    "tabs",
    "activeTab",
    "identity",
    "*://*/"
  ],
  "icons": {
    "128": "images/plugin_icon.png"
  },
  "browser_action": {
    "default_title": "E2T",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
