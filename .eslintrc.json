{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console":0,
        "semi": [
            "error",
            "always"
        ],
        "camelcase":"warn",
        "indent": ["warn", 4, {"SwitchCase": 1}],
        "no-unused-vars": [
            "warn",
            {
                "varsIgnorePattern": "^_$",
                "argsIgnorePattern": "^_$" 
            }
        ]
    }
}